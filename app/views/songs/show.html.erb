<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @song.name %>
</p>

<p>
  <strong>Chords:</strong>
  <%= @song.chords %>
</p>

<p>
<strong>Modulos:</strong>
<%=params[:modulo]%>
</p>

<strong>Analyse:</strong>
<% @analyses.each do |analyse| %>
  <p>
    <table>
      <tr>
        <th>Modulo</th>
        <th></th>
        <% @chords.each_with_index do |a, i| %>           
          <th><%= a%></th>
        <% end %>

      </tr>
      <tr>
        <td><%= analyse['m'] %></td>
        <% analyse['a'].each do |a| %>           
          <td><%= a%></td>
        <% end %>
      </tr>
    </table>
    
  </p>
<% end %>

<p>
  <strong>Chords Split:</strong>
  <p>
  <% @chords.each do |c| %>
    <%= c %><br/>
  <% end %>
  </p>
</p>

<%= link_to 'Edit', edit_song_path(@song) %> |
<%= link_to 'Back', songs_path %>
